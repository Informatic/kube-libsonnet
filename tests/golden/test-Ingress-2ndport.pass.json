         "labels": {
            "name": "test-Ingress-fail-pod"
         },
         "name": "test-Ingress-fail-pod"
      },
      "spec": {
         "containers": [
            {
               "args": [ ],
               "env": [ ],
               "image": "nginx:1.12",
               "imagePullPolicy": "IfNotPresent",
               "name": "test-Ingress-fail",
               "ports": [
                  {
                     "containerPort": 80,
                     "name": "http"
                  },
                  {
                     "containerPort": 9099,
                     "name": "metrics"
                  }
